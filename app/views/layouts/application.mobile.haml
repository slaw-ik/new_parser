    %script{:src => "https://d10ajoocuyu32n.cloudfront.net/codiqa.ext.js"}

    %script{:src => "http://maps.google.com/maps/api/js?sensor=true", :type => "text/javascript"}
    %script{:src => "http://jquery-ui-map.googlecode.com/svn/trunk/ui/jquery.ui.map.js", :type => "text/javascript"}
